 //8. Function to return the max digit in int
int max(int m) {
	int max = INT_MIN,col=0;
	
	for (; m > 0;) {
		col = m % 10;
	
	if (col>max) {
		max = col;
	
		m /= 10;
	}
	}
	return max;
}
