//11.Write a program to input elements in an array and find two numbers whose sum 
//equals the last element in the array

int arr[10], x;

for (int x = 0; x < 10;x++) {
	cin >> arr[x];
}
x = arr[9];
for (int i = 0; i < 10;i++) {

	for (int j = i+1; j < 10;j++) {
		if (arr[i] +arr[j]==x) {
			cout << arr[i] << "+" << arr[j] << "= " << x<<endl;
		}

	}

}






