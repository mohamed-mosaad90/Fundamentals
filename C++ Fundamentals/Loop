//11- 11. Write a program to take 10 numbers from the user and find the second max.

int num,max=INT_MIN,sec=INT_MIN;

for (int x = 1; x <= 10;x++) {
	cin >> num;
	if (max<num) {
		sec = max;
		max = num;
		
	
	}
	if (num<max&&num>sec) {
		sec = num;
	}
}
cout <<"result" << sec<<endl;
cout <<"result max" << max<<endl;



