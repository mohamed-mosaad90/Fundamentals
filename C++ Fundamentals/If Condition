// 18.13.Write a program that take 3 integer from user then print the max element and
//the min element.

int x, y, z, max = INT_MAX, min = INT_MIN;
cout << "enter 3 integer";
cin >> x >> y >> z;
//max
if (x > y && x > z)
{
	max = x
		;

}
else if (y > z && y > x) {
	max = y;
}
else
{
	max = z;
}
//min
if (x < y && x < z)
{
	min = x
		;

}
else if (y < z && y < x) {
	min = y;
}
else
{
	min = z;
}
cout << "max" << max<<endl;
cout << "min" << min;

